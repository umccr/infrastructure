{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "",
            "Effect": "Allow",
            "Action": [
              "s3:PutObject",
              "s3:ListBucket",
              "s3:GetBucketLocation"
            ],
            "Resource": [
              "arn:aws:s3:::${bucket_name}",
              "arn:aws:s3:::${bucket_name}/*"
            ]
        }
    ]
}
